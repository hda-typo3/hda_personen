		templateRootPath = EXT:hda_personen/Resources/Private/Templates/
		
		# cat=plugin.tx_hdapersonen/file; type=string; label= Path to partials root (FE)
		partialRootPath = EXT:hda_personen/Resources/Private/Partials/		
		
	}
       
	persistence {
	    # cat=plugin.tx_hdapersonen//; type=int+; label=Storage PID der LDAP Eintr√§ge
	    storagePid =
		# cat=plugin.tx_hdapersonen//; type=int+; label=Recursive
	    recursive = 4	    
	}
	
	settings {
        # cat=plugin.tx_hdapersonen//; type=string; label= Second part of the encryption key
